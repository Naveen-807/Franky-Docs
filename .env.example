# Required
GOOGLE_SERVICE_ACCOUNT_JSON=/absolute/path/to/service-account.json
DOCWALLET_MASTER_KEY=change_me_to_a_long_random_string

# Server
HTTP_PORT=8787
PUBLIC_BASE_URL=http://localhost:8787

# Discovery
DOCWALLET_DOC_ID=
DOCWALLET_DISCOVER_ALL=0
DOCWALLET_NAME_PREFIX=[DocWallet]
POLL_INTERVAL_MS=5000
DISCOVERY_INTERVAL_MS=60000

# Arc (EVM) - recommended for demo
ARC_ENABLED=1
ARC_RPC_URL=https://rpc.testnet.arc.network
ARC_USDC_ADDRESS=0x3600000000000000000000000000000000000000

# Circle dev-controlled wallets (optional for demo)
CIRCLE_ENABLED=0
CIRCLE_API_KEY=
CIRCLE_ENTITY_SECRET=
CIRCLE_WALLET_SET_ID=
CIRCLE_BLOCKCHAIN=ARC-TESTNET
CIRCLE_ACCOUNT_TYPE=EOA

# Sui / DeepBook (optional for demo)
DEEPBOOK_ENABLED=0
SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# ENS policy (optional)
ENS_RPC_URL=https://mainnet.infura.io/v3/YOUR_KEY

# Yellow NitroRPC (optional for demo)
YELLOW_ENABLED=0
YELLOW_RPC_URL=wss://your-clearnode.example
YELLOW_WS_URL=
YELLOW_APP_NAME=DocWallet

# WalletConnect (optional for demo)
WALLETCONNECT_ENABLED=0
WALLETCONNECT_PROJECT_ID=
WALLETCONNECT_RELAY_URL=

# Polling intervals
BALANCE_POLL_INTERVAL_MS=60000
SCHEDULER_INTERVAL_MS=30000
